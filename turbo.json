{
   "$schema": "https://turbo.build/schema.json",
   "pipeline": {
      "engine#build": {
         "dependsOn": ["sdk#build", "adapter-squid#build", "adapter-ccxt#build"]
      },
      "build": {
         "dependsOn": ["^build"],
         "outputs": ["dist/**", "tsconfig.tsbuildinfo"]
      },
      "test": {
         "dependsOn": ["build"],
         "outputs": ["coverage/**"]
      }
   },
   "globalDependencies": [
      "tsconfig.base.json",
      "tsconfig.build.json",
      "tsconfig.json"
   ]
}
